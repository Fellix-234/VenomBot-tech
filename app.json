{
  "name": "VenomBot-Tech",
  "description": "Professional WhatsApp Bot built with Baileys",
  "repository": "https://github.com/Fellix-234/VenomBot-tech",
  "keywords": [
    "whatsapp",
    "bot",
    "baileys",
    "whatsapp-bot"
  ],
  "env": {
    "PREFIX": {
      "description": "Bot command prefix",
      "value": "!"
    },
    "BOT_NAME": {
      "description": "Bot name",
      "value": "VenomBot"
    },
    "OWNER_NUMBER": {
      "description": "Bot owner WhatsApp number (with country code)",
      "value": "0"
    },
    "TIMEZONE": {
      "description": "Bot timezone",
      "value": "Africa/Nairobi"
    },
    "AUTO_READ": {
      "description": "Auto read messages",
      "value": "true"
    },
    "AUTO_TYPING": {
      "description": "Show typing indicator",
      "value": "true"
    },
    "AUTO_REACT": {
      "description": "Auto react to messages",
      "value": "false"
    },
    "USE_JSON_DB": {
      "description": "Use JSON database",
      "value": "true"
    }
  }
}
